local MyTab = Window:CreateTab("boyandgirltower", nil) -- Title, Image

local Button = MyTab:CreateButton({
   Name = "get all coin in map",
   Callback = function()
   -- The function that takes place when the button is pressed
      local coinFolder = workspace["Coin System"].Coins

local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")

for _, coin in ipairs(coinFolder:GetChildren()) do
    if coin:IsA("BasePart") then
        hrp.CFrame = coin.CFrame
        task.wait(0.2)
    end
end
   end,
})

local Button = MyTab:CreateButton({
   Name = "Teleport Finish",
   Callback = function()
local player = game.Players.LocalPlayer
local char = player.Character or player.CharacterAdded:Wait()
local hrp = char:WaitForChild("HumanoidRootPart")

local target = workspace:GetChildren()[38].Model:FindFirstChild("Lobby")

if target and target:IsA("BasePart") then
    hrp.CFrame = target.CFrame + Vector3.new(0, 3, 0)
else
    warn("Không tìm thấy Lobby")
end
   end,
})

