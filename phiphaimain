-- 5. workspace.Slap.Giver  (Give)
local slapGiver = workspace:WaitForChild("Slap"):WaitForChild("Giver")
tpTo(slapGiver)

task.wait(0.1)

-- 6. workspace:GetChildren()[19].Giver  (Giver)  <-- M·ªöI ƒê∆ØA V√ÄO
local f19 = workspace:GetChildren()[19]
if f19 and f19:FindFirstChild("Giver") then
    tpTo(f19.Giver)
end

task.wait(0.1)

-- 7. workspace.Model:GetChildren()[7] (object)
local m7 = workspace:WaitForChild("Model"):GetChildren()[7]
if m7 and m7:IsA("BasePart") then
    tpTo(m7)
end
   end,
})


