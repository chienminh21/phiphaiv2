local Button = GameTab:CreateButton({
   Name = "esp",
   Callback = function()
    local RunService = game:GetService("RunService")

local RunService = game:GetService("RunService")

local GREEN = Color3.fromRGB(0,255,0)
local RED   = Color3.fromRGB(255,0,0)
local BLUE  = Color3.fromRGB(0,170,255)

-- G·∫Øn Highlight ESP
local function applyESP(target, color)
    if not target or target:FindFirstChild("ESP_APPLIED") then return end

    local tag = Instance.new("BoolValue")
    tag.Name = "ESP_APPLIED"
    tag.Parent = target

    local hl = Instance.new("Highlight")
    hl.FillColor = color
    hl.OutlineColor = color
    hl.FillTransparency = 0.4
    hl.OutlineTransparency = 0
    hl.Enabled = true
    hl.Parent = target
end

RunService.Heartbeat:Connect(function()
    local hum = workspace:FindFirstChild("Hum")
    if not hum then return end

    -- üü¢ Man1 -> Man24 (tr·ª´ Man7, Man17)
    for i = 1, 24 do
        if i ~= 7 and i ~= 17 then
            local manFolder = hum:FindFirstChild("Man"..i)
            if manFolder then
                local man1 = manFolder:FindFirstChild("Man1")
                if man1 then
                    applyESP(man1, GREEN)
                end
            end
        end
    end

    -- üî¥ Man7
    local man7 = hum:FindFirstChild("Man7")
    if man7 then
        local man1 = man7:FindFirstChild("Man1")
        if man1 then
            applyESP(man1, RED)
        end
    end

    -- üî¥ Man17
    local man17 = hum:FindFirstChild("Man17")
    if man17 then
        local man1 = man17:FindFirstChild("Man1")
        if man1 then
            applyESP(man1, RED)
        end
    end

    -- üü¢ Man25santa
    local santa = hum:FindFirstChild("Man25santa")
    if santa then
        local man1 = santa:FindFirstChild("Man1")
        if man1 then
            applyESP(man1, GREEN)
        end
    end

    -- üî¥ Anom1 -> Anom13
    for i = 1, 13 do
        local anomFolder = hum:FindFirstChild("Anom"..i)
        if anomFolder then
            local man1 = anomFolder:FindFirstChild("Man1")
            if man1 then
                applyESP(man1, RED)
            end
        end
    end

    -- üî¥ Scream1 -> Scream13
    for i = 1, 13 do
        local scream = hum:FindFirstChild("Scream"..i)
        if scream then
            applyESP(scream, RED)
        end
    end

    -- üî¥ Fali
    local fali = workspace:FindFirstChild("Fail")
    if fali then
        applyESP(fali, RED)
    end

    -- üîµ Internte.Man1
    local internte = hum:FindFirstChild("Internte")
    if internte then
        local man1 = internte:FindFirstChild("Man1")
        if man1 then
            applyESP(man1, BLUE)
        end
    end

    -- üîµ ManAN1.Man1
    local manAN1 = hum:FindFirstChild("ManAN1")
    if manAN1 then
        local man1 = manAN1:FindFirstChild("Man1")
        if man1 then
            applyESP(man1, BLUE)
        end
    end
end)
   end,
})

